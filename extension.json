{
	"name": "RequestCustomDomain",
	"author": [
		"Universal Omega",
		"Agent Isai",
		"Reception123",
		"Alex",
		"MacFan4000"
	],
	"url": "https://github.com/miraheze/RequestCustomDomain",
	"descriptionmsg": "requestcustomdomain-desc",
	"namemsg": "requestcustomdomain-extensionname",
	"license-name": "GPL-3.0-or-later",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.43.0",
		"platform": {
			"php": ">= 8.2"
		}
	},
	"MessagesDirs": {
		"RequestCustomDomain": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"RequestCustomDomainAliases": "RequestCustomDomainAliases.php"
	},
	"AutoloadNamespaces": {
		"Miraheze\\RequestCustomDomain\\": "includes/"
	},
	"TestAutoloadNamespaces": {
		"Miraheze\\RequestCustomDomain\\Tests\\": "tests/phpunit/"
	},
	"GroupPermissions": {
		"user": {
			"request-custom-domain": true
		}
	},
	"AvailableRights": [
		"handle-custom-domain-requests",
		"request-custom-domain"
	],
	"JobClasses": {
		"RequestCustomDomainCFAddJob": {
			"class": "Miraheze\\RequestCustomDomain\\Jobs\\RequestCustomDomainCFAddJob",
			"services": [
				"HttpRequestFactory",
				"RequestCustomDomainConfig",
				"RequestCustomDomainLogger",
				"RequestCustomDomainManager"
			],
			"needsPage": false
		}
	},
	"LogActionsHandlers": {
		"requestcustomdomain/*": "LogFormatter"
	},
	"LogHeaders": {
		"requestcustomdomain": "requestcustomdomain-log-header"
	},
	"LogNames": {
		"requestcustomdomain": "requestcustomdomain-log-name"
	},
	"LogTypes": [
		"requestcustomdomain"
	],
	"ActionFilteredLogs": {
		"requestcustomdomain": {
			"request": [
				"request"
			],
			"statusupdate": [
				"statusupdate"
			]
		}
	},
	"SpecialPages": {
		"RequestCustomDomain": {
			"class": "Miraheze\\RequestCustomDomain\\Specials\\SpecialRequestCustomDomain",
			"services": [
				"ConnectionProvider",
				"MimeAnalyzer",
				"RepoGroup",
				"RequestCustomDomainManager",
				"UserFactory"
			]
		},
		"RequestCustomDomainQueue": {
			"class": "Miraheze\\RequestCustomDomain\\Specials\\SpecialRequestCustomDomainQueue",
			"services": [
				"ConnectionProvider",
				"PermissionManager",
				"RequestCustomDomainManager",
				"UserFactory"
			]
		}
	},
	"Hooks": {
		"BeforeCreateEchoEvent": {
			"handler": "Main"
		},
		"GetAllBlockActions": {
			"handler": "Main"
		},
		"LoadExtensionSchemaUpdates": {
			"handler": "Installer"
		},
		"UserGetReservedNames": {
			"handler": "Main"
		}
	},
	"HookHandlers": {
		"Installer": {
			"class": "Miraheze\\RequestCustomDomain\\HookHandlers\\Installer"
		},
		"Main": {
			"class": "Miraheze\\RequestCustomDomain\\HookHandlers\\Main",
			"services": [
				"ConnectionProvider"
			]
		}
	},
	"ResourceModules": {
		"ext.requestcustomdomain.oouiform": {
			"scripts": "ext.requestcustomdomain.oouiform.ooui.js",
			"dependencies": [
				"mediawiki.storage",
				"oojs-ui-widgets"
			]
		},
		"ext.requestcustomdomain.oouiform.styles": {
			"styles": "ext.requestcustomdomain.oouiform.ooui.less"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "RequestCustomDomain/modules"
	},
	"config": {
		"RequestCustomDomainCloudflareConfig": {
			"description": "Array. Cloudflare configuration array, including API Key and Zone ID.",
			"value": {
				"apikey": null,
				"tlsversion": "1.3",
				"zoneid": null
			}
		},
		"RequestCustomDomainDisallowedDomains": {
			"value": [],
			"description": "Array. Domains that are not allowed to be requested via RequestCustomDomain."
		},
		"RequestCustomDomainHelpUrl": {
			"value": "",
			"description": "Full URL. If set, adds a help URL to Special:RequestCustomDomain."
		},
		"RequestCustomDomainUsersNotifiedOnAllRequests": {
			"value": [],
			"description": "Array of usernames to send email notifications to for all requests. Users listed here must have a verified email address on their account. This doesn't do anything if the Echo extension is not installed."
		}
	},
	"ConfigRegistry": {
		"RequestCustomDomain": "MediaWiki\\Config\\GlobalVarConfig::newInstance"
	},
	"DefaultUserOptions": {
		"echo-subscriptions-email-requestcustomdomain-new-request": true,
		"echo-subscriptions-email-requestcustomdomain-request-comment": true,
		"echo-subscriptions-email-requestcustomdomain-request-status-update": true,
		"echo-subscriptions-web-requestcustomdomain-new-request": false,
		"echo-subscriptions-web-requestcustomdomain-request-comment": true,
		"echo-subscriptions-web-requestcustomdomain-request-status-update": true
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"DatabaseVirtualDomains": [
		"virtual-requestcustomdomain"
	],
	"manifest_version": 2
}

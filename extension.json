{
	"name": "RequestSSL",
	"author": [
		"Universal Omega",
		"Agent Isai",
		"Reception123",
		"Alex",
		"MacFan4000"
	],
	"url": "https://github.com/miraheze/RequestSSL",
	"descriptionmsg": "requestssl-desc",
	"namemsg": "requestssl-extensionname",
	"license-name": "GPL-3.0-or-later",
	"type": "specialpage",
	"requires": {
		"MediaWiki": ">= 1.43.0"
	},
	"MessagesDirs": {
		"RequestSSL": [
			"i18n"
		]
	},
	"ExtensionMessagesFiles": {
		"RequestSSLAliases": "RequestSSLAliases.php"
	},
	"AutoloadNamespaces": {
		"Miraheze\\RequestSSL\\": "includes/"
	},
	"TestAutoloadNamespaces": {
		"Miraheze\\RequestSSL\\Tests\\": "tests/phpunit/"
	},
	"GroupPermissions": {
		"user": {
			"request-custom-domain": true
		}
	},
	"AvailableRights": [
		"handle-custom-domain-requests",
		"request-custom-domain"
	],
	"JobClasses": {
		"RequestSSLCFAddJob": {
			"class": "Miraheze\\RequestSSL\\Jobs\\RequestSSLCFAddJob",
			"services": [
				"HttpRequestFactory",
				"RequestSSLConfig",
				"RequestSSLLogger",
				"RequestSSLManager"
			],
			"needsPage": false
		}
	},
	"LogActionsHandlers": {
		"requestcustomdomain/*": "LogFormatter"
	},
	"LogHeaders": {
		"requestcustomdomain": "requestcustomdomain-log-header"
	},
	"LogNames": {
		"requestcustomdomain": "requestcustomdomain-log-name"
	},
	"LogTypes": [
		"requestcustomdomain"
	],
	"ActionFilteredLogs": {
		"requestcustomdomain": {
			"request": [
				"request"
			],
			"statusupdate": [
				"statusupdate"
			]
		}
	},
	"SpecialPages": {
		"RequestCustomDomain": {
			"class": "Miraheze\\RequestSSL\\Specials\\SpecialRequestCustomDomain",
			"services": [
				"ConnectionProvider",
				"MimeAnalyzer",
				"RequestSSLManager",
				"RepoGroup",
				"UserFactory"
			]
		},
		"RequestCustomDomainQueue": {
			"class": "Miraheze\\RequestSSL\\Specials\\SpecialRequestCustomDomainQueue",
			"services": [
				"ConnectionProvider",
				"RequestSSLManager",
				"PermissionManager",
				"UserFactory"
			]
		}
	},
	"Hooks": {
		"BeforeCreateEchoEvent": {
			"handler": "Main"
		},
		"GetAllBlockActions": {
			"handler": "Main"
		},
		"LoadExtensionSchemaUpdates": {
			"handler": "Installer"
		},
		"UserGetReservedNames": {
			"handler": "Main"
		}
	},
	"HookHandlers": {
		"Installer": {
			"class": "Miraheze\\RequestSSL\\Hooks\\Handlers\\Installer"
		},
		"Main": {
			"class": "Miraheze\\RequestSSL\\Hooks\\Handlers\\Main",
			"services": [
				"ConnectionProvider"
			]
		}
	},
	"ResourceModules": {
		"ext.requestssl.oouiform": {
			"scripts": "ext.requestssl.oouiform.ooui.js",
			"dependencies": [
				"mediawiki.storage",
				"oojs-ui-widgets"
			]
		},
		"ext.requestssl.oouiform.styles": {
			"styles": "ext.requestssl.oouiform.ooui.less"
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "RequestSSL/modules"
	},
	"config": {
		"RequestSSLCloudflareConfig": {
			"description": "Array. Cloudflare configuration array, including API Key and Zone ID.",
			"value": {
				"apikey": null,
				"tlsversion": "1.3",
				"zoneid": null
			}
		},
		"RequestSSLDisallowedDomains": {
			"value": [],
			"description": "Array. Domains that are not allowed to be requested via RequestSSL."
		},
		"RequestSSLHelpUrl": {
			"value": "",
			"description": "Full URL. If set, adds a help URL to Special:RequestSSL."
		},
		"RequestSSLUsersNotifiedOnAllRequests": {
			"value": [],
			"description": "Array of usernames to send email notifications to for all requests. Users listed here must have a verified email address on their account. This doesn't do anything if the Echo extension is not installed."
		}
	},
	"ConfigRegistry": {
		"RequestSSL": "MediaWiki\\Config\\GlobalVarConfig::newInstance"
	},
	"DefaultUserOptions": {
		"echo-subscriptions-email-requestssl-new-request": true,
		"echo-subscriptions-email-requestssl-request-comment": true,
		"echo-subscriptions-email-requestssl-request-status-update": true,
		"echo-subscriptions-web-requestssl-new-request": false,
		"echo-subscriptions-web-requestssl-request-comment": true,
		"echo-subscriptions-web-requestssl-request-status-update": true
	},
	"ServiceWiringFiles": [
		"includes/ServiceWiring.php"
	],
	"DatabaseVirtualDomains": [
		"virtual-requestssl"
	],
	"manifest_version": 2
}
